{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "RAW_BASE_URL": "readonly",
    "REPO_OWNER": "readonly",
    "REPO_NAME": "readonly",
    "allServices": "writable",
    "currentChecksHash": "writable",
    "filteredServices": "writable",
    "currentServiceOrg": "writable",
    "currentServiceRepo": "writable",
    "serviceWorkflowLoaded": "writable",
    "serviceWorkflowRuns": "writable",
    "serviceWorkflowFilterStatus": "writable",
    "serviceWorkflowPollInterval": "writable",
    "serviceWorkflowPollIntervalTime": "writable",
    "serviceDurationUpdateInterval": "writable",
    "fetchWithHybridAuth": "readonly",
    "showToast": "readonly",
    "loadWorkflowRunsForService": "readonly"
  },
  "rules": {
    "indent": "off",
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": ["warn"],
    "no-console": "off",
    "no-func-assign": "off"
  }
}
